{
  "name": "stoked",
  "version": "0.0.1-a.0",
  "description": "",
  "author": "Brian Stoker (w/ help from Claude)",
  "bin": {
    "stoked": "dist/main.js"
  },
  "main": "dist/main.js",
  "private": true,
  "license": "MIT",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=22.12.0"
  },
  "type": "module",
  "scripts": {
    "clean": "trash ./dist",
    "build": "swc ./src -d ./dist --delete-dir-on-start",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "fix-imports": "node src/fix-imports.mjs",
    "dev": "NODE_OPTIONS='--no-warnings --loader ts-node/esm' ts-node --project tsconfig.node.json src/main.ts",
    "debug": "NODE_OPTIONS='--no-warnings --loader ts-node/esm --experimental-specifier-resolution=node --inspect-brk' ts-node --project tsconfig.node.json src/main.ts",
    "debug:cmd": "NODE_OPTIONS='--no-warnings --experimental-specifier-resolution=node --inspect-brk --enable-source-maps' node src/debug-helper.mjs",
    "debug:mjs": "node --no-warnings --inspect-brk --enable-source-maps src/debug-helper.mjs",
    "debug:minimal": "node --no-warnings --inspect-brk --enable-source-maps src/debug-minimal.mjs",
    "debug:direct": "node --no-warnings --inspect-brk --enable-source-maps src/direct-debug.mjs",
    "debug:direct:no-inspect": "node --no-warnings src/direct-debug.mjs",
    "debug:simple": "node --no-warnings --inspect-brk --enable-source-maps src/simple-debug.mjs",
    "debug:simple:no-inspect": "node --no-warnings src/simple-debug.mjs",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "pnpm test:unit",
    "test:unit": "vitest run \"test/unit/\"",
    "test:integration": "vitest run \"test/integration/\"",
    "test:e2e": "playwright test",
    "test:full": "pnpm test:unit && pnpm test:integration && pnpm test:e2e",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:ui:cov": "vitest --ui --coverage=true",
    "test:ui:full": "pnpm test:unit:cov && pnpm test:integration:cov && pnpm test:ui:cov",
    "test:unit:cov": "rm -rf test/coverage/unit && VITEST_COVERAGE_DIR=test/coverage/unit vitest run \"test/unit/\" --coverage",
    "test:integration:cov": "rm -rf test/coverage/integration && VITEST_COVERAGE_DIR=test/coverage/integration vitest run \"test/integration/\" --coverage",
    "test:e2e:cov": "playwright test --reporter=html,json --output=test/playwright-report",
    "test:cov:gen": "mkdir -p test/reports test/coverage/unit test/coverage/integration && pnpm test:unit:cov && pnpm test:integration:cov && pnpm test:e2e:cov && echo 'Coverage reports available in: test/coverage/unit/, test/coverage/integration/ and test/playwright-report/'",
    "test:cov:combined": "mkdir -p test/reports test/coverage/unit test/coverage/integration && pnpm test:unit:cov && pnpm test:integration:cov && pnpm test:e2e:cov && cp -r test/coverage/unit test/reports/unit-coverage && cp -r test/coverage/integration test/reports/integration-coverage && cp -r test/playwright-report test/reports/e2e-coverage && [ -f test/playwright-report/playwright-report.json ] && cp test/playwright-report/playwright-report.json test/reports/playwright-results.json || echo 'Playwright report not found' && echo 'Combined reports available in test/reports/ directory'",
    "test:cov": "pnpm test:cov:combined && npx serve test/reports",
    "test:all": "pnpm test:unit && pnpm test:integration && pnpm test:e2e",
    "test:all:cov": "pnpm test:cov:combined && npx serve test/reports",
    "test:debug": "vitest --inspect-brk",
    "test:e2e:cli": "playwright test test/e2e/cli.spec.ts --output=test/playwright-report",
    "test:e2e:jsdocs": "playwright test test/e2e/jsdocs.spec.ts --output=test/playwright-report",
    "test:e2e:debug": "playwright test --debug --output=test/playwright-report",
    "test:cleanup": "node ./scripts/cleanup-test-dirs.js"
  },
  "dependencies": {
    "@actions/github": "^6.0.0",
    "@nestjs/common": "^11.0.11",
    "@nestjs/core": "^11.0.13",
    "@nestjs/cqrs": "^11.0.2",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.0.1",
    "@octokit/rest": "^21.1.1",
    "@octokit/types": "^13.8.0",
    "@types/js-yaml": "^4.0.9",
    "@vitest/ui": "^3.1.1",
    "axios": "^1.8.3",
    "bcrypt": "^5.1.1",
    "chalk": "^5.4.1",
    "class-validator": "^0.14.1",
    "commander": "^13.1.0",
    "dotenv": "^16.4.7",
    "form-data": "^4.0.2",
    "js-yaml": "^4.1.0",
    "nest-commander": "^3.17.0",
    "octokit": "^4.1.2",
    "ollama": "^0.5.14",
    "process": "^0.11.10",
    "readline": "^1.3.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/runtime": "^7.24.8",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@jest/globals": "^29.7.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@playwright/test": "^1.51.1",
    "@pnpm/link-bins": "^1000.0.8",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "3.1.1",
    "c8": "^10.1.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "ignore": "^7.0.0",
    "jest": "^29.7.0",
    "node-fetch": "3.3.2",
    "playwright": "^1.51.1",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.20.0",
    "vitest": "^3.1.1"
  }
}
